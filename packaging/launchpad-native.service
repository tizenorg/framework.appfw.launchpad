[Unit]
Description=Start native launchpad daemon
After=dbus.service vconf-setup.service ac.service

[Service]
EnvironmentFile=/run/tizen-mobile-env
ExecStart=/usr/bin/launchpad-native "                                                                                                                                                                                                         "
TimeoutStopSec=3s
Restart=always
RestartSec=0

[Install]
WantedBy=graphical.target
